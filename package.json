{
  "name": "@josephuspaye/launch-start-app",
  "version": "0.0.0",
  "description": "Launch apps in the Start Menu from @josephuspaye/start-apps.",
  "main": "dist/main.js",
  "repository": "https://github.com/JosephusPaye/launch-start-app.git",
  "author": "Josephus Paye II",
  "license": "MIT",
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"*.{js,json,md}\"",
    "test": "echo nah"
  },
  "dependencies": {
    "@josephuspaye/powershell": "^0.2.0"
  },
  "devDependencies": {
    "@josephuspaye/start-apps": "^0.1.0",
    "@types/node": "^14.14.10",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true
  }
}
